#include<stdio.h>
#include<string.h>
void main()
{
    char pass[20];
    printf("Enter your Password:");
    scanf("%s",pass);
    int l=0,u=0,d=0,s=0;
    if(strlen(pass)>=8)
    {
        for( int i=0;i<strlen(pass);i++)
        {
            if(islower(pass[i]))
              l++;
            else if(isupper(pass[i]))
               u++;
            else if(isdigit(pass[i]))
                d++;
            else
                s++;
        }
        if(l>0 && u>0 && d>0 && s>0)
        {
            printf("Strong Password");

        }
        else{
            printf("Weak password");
        }

        
    }
    else{
        printf("Weak password,because less number of charcters.");
    }
}
