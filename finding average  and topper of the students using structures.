#include<stdio.h>
struct student
{
    char name[50];
    int marks[5];
    float AVG;
};
void main()
{
    struct student s[3];
    for(int i=0;i<3;i++)
    {
       printf("Enter student name:");
       scanf("%s",s[i].name);
       printf("Enter five subjects marks of the student:\n");
       int sum=0;
       for(int j=0;j<5;j++)
       {
        scanf("%d",&s[i].marks[j]);
        sum+=s[i].marks[j];
       }
       s[i].AVG=(float) sum/5.0;
    }
    int maxind=0;
     float maxavg=s[0].AVG;
    for( int i=1;i<3;i++)
    {
        if(s[i].AVG>maxavg)
        {
            maxind=i;
            maxavg=s[i].AVG;
        } 
    }
    printf("Topper is %s\n",s[maxind].name);
    printf("Average is %f",s[maxind].AVG);

   
}
