#include <stdio.h>
int main()
{
    int n,array[50],sum=0,i;
    printf("Enter number of students in the class:");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        printf("Enter marks of the student:");
        scanf("%d",&array[i]);
        sum+=array[i];
        
    }
    printf("Sum of all the students marks is %d",sum);
    return 0;
    
}
