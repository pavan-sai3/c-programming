#include<stdio.h>
int dgsum(int);
int dgsum(int n)

{    int res=0;
    while(n>0)
     {
      res= res+n%10;
      n=n/10;
     }

    return res;
}
void main()
{
    int arr[10]={12,22,33,44,55,66,77,88,99,100};
    int sum=0;
    for( int i=0;i<10;i++)
    {
        sum+=dgsum(arr[i]);
    }
    printf("The sum is:%d",sum);
}
