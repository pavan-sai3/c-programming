//check whether number is prime or not?

#include <stdio.h>
int n,i,flag=1;
int main()
{
  printf("enter n number:");
  scanf("%d",&n);
  for(i=2;i<n;i++)
  {

    if(n%i==0)   
    {
      flag=0;
      break;
      
    }
  }

if(flag==0)
 {
     printf("not prime");
    }
else
 { 
     printf("prime");
  }
  return 0;
}
